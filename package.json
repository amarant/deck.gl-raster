{
  "name": "@amarant/deck.gl-raster",
  "description": "A collection of custom Deck.gl layers",
  "license": "MIT",
  "version": "0.3.3",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "webgl",
    "visualization",
    "geospatial",
    "layer",
    "satellite"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/amarant/deck.gl-raster.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "build-examples": "cd examples/raster-layer && yarn build && cd ../raster-mesh-layer && yarn build",
    "build-docs": "yarn build-examples && yarn clean-docs && cp -r examples/raster-layer/build docs/examples/raster-layer && cp -r examples/raster-mesh-layer/build docs/examples/raster-mesh-layer && mkdocs build",
    "clean-docs": "rm -rf docs/examples/raster-layer docs/examples/raster-mesh-layer",
    "deploy-docs": "yarn build-docs && mkdocs gh-deploy",
    "format": "prettier src/ --write"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0"
  },
  "peerDependencies": {
    "@deck.gl/core": "^9.0.0",
    "@deck.gl/layers": "^9.0.0",
    "@deck.gl/mesh-layers": "^9.0.0",
    "@luma.gl/constants": "^9.0.0",
    "@luma.gl/core": "^9.0.0",
    "@luma.gl/engine": "^9.0.0",
    "@luma.gl/shadertools": "^9.0.0",
    "@luma.gl/webgl": "^9.0.0"
  },
  "devDependencies": {
    "@deck.gl/core": "^9.0.0",
    "@deck.gl/layers": "^9.0.0",
    "@deck.gl/mesh-layers": "^9.0.0",
    "@luma.gl/constants": "^9.0.0",
    "@luma.gl/core": "^9.0.0",
    "@luma.gl/engine": "^9.0.0",
    "@luma.gl/shadertools": "^9.0.0",
    "@luma.gl/webgl": "^9.0.0",
    "@plutotcool/rollup-plugin-glsl": "^1.2.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/lodash.isequal": "^4.5.8",
    "prettier": "^3.2.5",
    "rollup": "^4.16.1",
    "rollup-plugin-glsl": "^1.3.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@4.1.1"
}
